{
  "name": "postmark-webhook",
  "version": "0.0.1",
  "description": "A webhook for Astro's postmark.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prebuild": "rm -rf dist; mkdir dist",
    "build": "esbuild index.js --bundle --minify --outfile=dist/index.js --platform=node",
    "postbuild": "cd dist && zip -r handler.zip index.js",
    "updateLambda": "aws lambda update-function-code --function-name dev-postmark-webhook --zip-file 'fileb://dist/handler.zip'"
  },
  "author": "ASTRO",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "esbuild": "^0.20.0",
    "express": "^4.18.2"
  }
}