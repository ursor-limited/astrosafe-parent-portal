{
  "name": "stripe-webhook",
  "version": "0.0.1",
  "description": "A webhook for Astro's subscriptions.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prebuild": "rm -rf dist; mkdir dist",
    "build": "esbuild index.js --bundle --minify --outfile=dist/index.js --platform=node",
    "postbuild": "cd dist && zip -r handler.zip index.js",
    "updateLambda": "aws lambda update-function-code --function-name stripe-webhook --zip-file 'fileb://dist/handler.zip'"
  },
  "author": "ASTRO",
  "license": "ISC",
  "dependencies": {
    "esbuild": "^0.20.0",
    "express": "^4.18.2",
    "stripe": "^14.15.0"
  }
}